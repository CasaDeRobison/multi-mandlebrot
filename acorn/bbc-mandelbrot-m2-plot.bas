1 MODE 2
10 CLS
100 FOR PY%=0 TO 21
110 FOR RX%=0 TO 31
115 PX% = 31 - RX%
120 XZ = PX%*3.5/32-2.5
130 YZ = PY%*2/22-1
140 X=0
150 Y=0
160 FOR I%=0 TO 14
170 IF X*X+Y*Y > 4 THEN GOTO 215
180 XT = X*X - Y*Y + XZ
190 Y = 2*X*Y + YZ
200 X = XT
210 NEXT I%
215 GCOL 0,(I%/2)-1
220 MOVE RX%*32,PY%*32+31:MOVE RX%*32+31,PY%*32:PLOT 85,RX%*32,PY%*32
230 MOVE RX%*32,PY%*32+31:MOVE RX%*32+31,PY%*32:PLOT 85,RX%*32+31,PY%*32+31
240 NEXT RX%
260 NEXT PY%
