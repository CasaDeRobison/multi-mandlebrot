1 MODE 2
10 W%=32:H%=22
100 FOR PY%=0 TO H%-1
110 FOR PX%=0 TO W%-1
120 XZ = PX%*3.5/W%-2.5
130 YZ = PY%*2/H%-1
140 X=0
150 Y=0
160 FOR I%=0 TO 14
170 IF X*X+Y*Y > 4 THEN GOTO 215
180 XT = X*X - Y*Y + XZ
190 Y = 2*X*Y + YZ
200 X = XT
210 NEXT I%
215 GCOL 0,(I%/2)-1
220 MOVE PX%*32,1024-(PY%*32+31):MOVE PX%*32+31,1024-PY%*32:PLOT 85,PX%*32,1024-PY%*32
230 MOVE PX%*32,1024-(PY%*32+31):MOVE PX%*32+31,1024-PY%*32:PLOT 85,PX%*32+31,1024-(PY%*32+31)
240 NEXT PX%
260 NEXT PY%
